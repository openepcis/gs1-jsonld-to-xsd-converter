            <xsd:element name="postOfficeBoxNumber" type="xsd:string"/>
            <xsd:element name="postalCode" type="xsd:string"/>
            <xsd:element name="postalName" type="xsd:string"/>
            <xsd:element name="streetAddress" type="xsd:string"/>
            <xsd:element name="streetAddressLine2" type="xsd:string"/>
            <xsd:element name="streetAddressLine3" type="xsd:string"/>
            <xsd:element name="streetAddressLine4" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="PriceSpecification">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A structured value representing a monetary amount, consisting of a value and currency code.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dutyFeeTaxAmount" type="xsd:float"/>
            <xsd:element name="dutyFeeTaxDescription" type="xsd:string"/>
            <xsd:element name="dutyFeeTaxRate" type="xsd:float"/>
            <xsd:element name="eligibleQuantity" type="QuantitativeValue"/>
            <xsd:element name="eligibleQuantityMaximum" type="QuantitativeValue"/>
            <xsd:element name="eligibleQuantityMinimum" type="QuantitativeValue"/>
            <xsd:element name="maxPrice" type="xsd:float"/>
            <xsd:element name="minPrice" type="xsd:float"/>
            <xsd:element name="price" type="xsd:float"/>
            <xsd:element name="priceCurrency" type="xsd:string"/>
            <xsd:element name="validFrom" type="xsd:dateTime"/>
            <xsd:element name="validThrough" type="xsd:dateTime"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Product">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Any item (product or service) upon which there is a need to retrieve pre-defined information and that may be priced, or ordered, or
