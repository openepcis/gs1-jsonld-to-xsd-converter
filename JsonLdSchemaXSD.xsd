            <xsd:element name="afterHoursContact" type="ContactPoint"/>
            <xsd:element name="baseLocation" type="Place"/>
            <xsd:element name="certification" type="CertificationDetails"/>
            <xsd:element name="contactPoint" type="ContactPoint"/>
            <xsd:element name="containedInPlace" type="Place"/>
            <xsd:element name="containsPlace" type="Place"/>
            <xsd:element name="digitalAddress" type="xsd:string"/>
            <xsd:element name="digitalLocationName" type="xsd:string"/>
            <xsd:element name="geo" type="Place"/>
            <xsd:element name="glnType" type="GLN_TypeCode"/>
            <xsd:element name="globalLocationNumber" type="xsd:string"/>
            <xsd:element name="leasedFrom" type="Organization"/>
            <xsd:element name="leasedTo" type="Organization"/>
            <xsd:element name="locationGLN" type="xsd:string"/>
            <xsd:element name="locationHistory" type="LocationStatusHistory"/>
            <xsd:element name="locationRole" type="LocationRoleType"/>
            <xsd:element name="managedBy" type="Organization"/>
            <xsd:element name="managedFor" type="Organization"/>
            <xsd:element name="occupiedBy" type="Organization"/>
            <xsd:element name="ownedBy" type="Organization"/>
            <xsd:element name="physicalLocationName" type="xsd:string"/>
            <xsd:element name="primaryLocationOf" type="Organization"/>
            <xsd:element name="relatedOrganization" type="Organization"/>
            <xsd:element name="replacedByPlace" type="Place"/>
            <xsd:element name="replacedPlace" type="Place"/>
            <xsd:element name="responsibleOrganization" type="Organization"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="PostalAddress">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The location at which a particular organization or person may be found or reached.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="addressCountry" type="Country"/>
            <xsd:element name="addressLocality" type="xsd:string"/>
