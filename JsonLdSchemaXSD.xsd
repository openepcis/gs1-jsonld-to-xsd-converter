            <xsd:element name="seeker" type="Organization"/>
            <xsd:element name="seller" type="Organization"/>
            <xsd:element name="warranty" type="WarrantyPromise"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="OrganicClaimDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A set of organic claim details for the product.</xsd:documentation>
