        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Organization">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An Organization is any legal or physical entity involved at any point in any supply chain and upon which there is a need to retrieve
                predefined information. An Organization is uniquely identified by a Global Location Number (GLN).
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="additionalOrganizationID" type="OrganizationID_Details"/>
            <xsd:element name="additionalOrganizationIdentificationTypeValue" type="xsd:string"/>
            <xsd:element name="address" type="PostalAddress"/>
            <xsd:element name="afterHoursContact" type="ContactPoint"/>
            <xsd:element name="certification" type="CertificationDetails"/>
            <xsd:element name="contactPoint" type="ContactPoint"/>
            <xsd:element name="department" type="xsd:string"/>
            <xsd:element name="glnType" type="GLN_TypeCode"/>
            <xsd:element name="globalLocationNumber" type="xsd:string"/>
            <xsd:element name="hasPrimaryLocation" type="Place"/>
            <xsd:element name="lesseeOf" type="Place"/>
            <xsd:element name="lessorFor" type="Place"/>
            <xsd:element name="location" type="Place"/>
            <xsd:element name="makesOffer" type="Offer"/>
            <xsd:element name="managedBy" type="Organization"/>
            <xsd:element name="manages" type="Organization"/>
            <xsd:element name="occupies" type="Place"/>
            <xsd:element name="organizationHistory" type="OrganizationStatusHistory"/>
            <xsd:element name="organizationName" type="xsd:string"/>
            <xsd:element name="organizationRole" type="OrganizationRoleType"/>
            <xsd:element name="ownedBy" type="Organization"/>
            <xsd:element name="owns" type="Organization"/>
            <xsd:element name="parentOrganization" type="Organization"/>
            <xsd:element name="partyGLN" type="xsd:string"/>
            <xsd:element name="replacedByOrganization" type="Organization"/>
            <xsd:element name="replacedOrganization" type="Organization"/>
            <xsd:element name="responsibleForLocation" type="Place"/>
            <xsd:element name="subOrganization" type="Organization"/>
            <xsd:element name="usesManagedLocation" type="Place"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="OrganizationID_Details">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Government bodies, trade organisations, and other parties issue identifiers that are associated to legal entities and/or functions.
                Linking these identifiers to GLN and one another supports consolidating records, mapping related, collaborative identifiers, enhances search ability and enables
                more efficient transitions between identifiers. This class provides a mechanism for connecting and sharing party identifiers.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="organizationID" type="xsd:string"/>
            <xsd:element name="organizationID_Qualifier" type="xsd:string"/>
            <xsd:element name="organizationID_Type" type="OrganizationID_Type"/>
            <xsd:element name="organizationID_URI" type="xsd:anyURI"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="OrganizationStatusHistory">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Over the lifetime of an organisation, it may go through periods of being active and inactive. gs1:OrganizationStatusHistory allows
                changes in status to be communicated in advance and tracked over time.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="organizationStatus" type="StatusType"/>
            <xsd:element name="statusTimestamp" type="xsd:dateTime"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="PackagingDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Details on packaging for a product for example packaging type (bottle), materials, features, recycling, etc..</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="hasReturnablePackageDeposit" type="ReturnablePackageDepositDetails"/>
            <xsd:element name="packagingFeature" type="PackagingFeatureCode"/>
            <xsd:element name="packagingFunction" type="PackagingFunctionCode"/>
            <xsd:element name="packagingMaterial" type="PackagingMaterialDetails"/>
            <xsd:element name="packagingRecyclingProcessType" type="PackagingRecyclingProcessTypeCode"/>
            <xsd:element name="packagingRecyclingScheme" type="PackagingRecyclingSchemeCode"/>
            <xsd:element name="packagingShape" type="PackagingShapeCode"/>
            <xsd:element name="packagingType" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="PackagingMaterialDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Information on any material used for packaging.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="packagingMaterialCompositionQuantity" type="QuantitativeValue"/>
            <xsd:element name="packagingMaterialThickness" type="QuantitativeValue"/>
            <xsd:element name="packagingMaterialType" type="PackagingMaterialTypeCode"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Place">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Entities that have a somewhat fixed, physical location.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="additionalLocationID" type="LocationID_Details"/>
