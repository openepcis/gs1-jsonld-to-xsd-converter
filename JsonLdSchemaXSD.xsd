    <xsd:complexType name="ProductYieldDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A grouping of properties related to the yield of a food or beverage product according to a specified type of preparation.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="productYield" type="QuantitativeValue"/>
            <xsd:element name="productYieldType" type="ProductYieldTypeCode"/>
            <xsd:element name="productYieldVariationPercentage" type="xsd:float"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="QuantitativeValue">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A point value or interval for product characteristics and other purposes. A unit of measurement is also specified.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="unitCode" type="xsd:string"/>
            <xsd:element name="value" type="xsd:float"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="ReferencedFileDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Provides URL and other information on a referenced electronic file.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="fileLanguageCode" type="xsd:string"/>
            <xsd:element name="filePixelHeight" type="xsd:integer"/>
            <xsd:element name="filePixelWidth" type="xsd:integer"/>
            <xsd:element name="referencedFileEffectiveEndDateTime" type="xsd:dateTime"/>
            <xsd:element name="referencedFileEffectiveStartDateTime" type="xsd:dateTime"/>
            <xsd:element name="referencedFileSize" type="QuantitativeValue"/>
            <xsd:element name="referencedFileType" type="ReferencedFileTypeCode"/>
            <xsd:element name="referencedFileURL" type="xsd:anyURI"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="ReturnablePackageDepositDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Details of the deposit for returnable packaging for a product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="returnablePackageDepositAmount" type="PriceSpecification"/>
