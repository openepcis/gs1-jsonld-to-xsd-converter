            <xsd:element name="textileMaterialDescription" type="xsd:string"/>
            <xsd:element name="textileMaterialPercentage" type="xsd:float"/>
            <xsd:element name="textileMaterialThreadCount" type="xsd:string"/>
            <xsd:element name="textileMaterialWeight" type="QuantitativeValue"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="TypeCode">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A code list.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
