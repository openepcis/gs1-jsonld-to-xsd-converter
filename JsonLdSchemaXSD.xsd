            <xsd:element name="awardPrize" type="AwardPrizeDetails"/>
            <xsd:element name="bestBeforeDate" type="xsd:date"/>
            <xsd:element name="brand" type="Brand"/>
            <xsd:element name="brandOwner" type="Organization"/>
