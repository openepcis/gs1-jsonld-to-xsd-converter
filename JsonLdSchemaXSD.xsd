    <xsd:complexType name="WarrantyPromise">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An assurance that the product is reliable and that repairs or replacement will be done free of charge within a given time limit and
                under certain conditions in the event of a defect.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="durationOfWarranty" type="QuantitativeValue"/>
            <xsd:element name="warrantyScopeDescription" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="WearableProduct">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Products that are worn on the body.</xsd:documentation>
