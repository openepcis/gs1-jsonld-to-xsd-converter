        <xsd:complexContent>
            <xsd:extension base="Product">
                <xsd:sequence>
                    <xsd:element name="consumerLifestage" type="ConsumerLifestageCode"/>
                    <xsd:element name="isPatterned" type="NonbinaryLogicCode"/>
                    <xsd:element name="isThermal" type="NonbinaryLogicCode"/>
                    <xsd:element name="isWaterproof" type="NonbinaryLogicCode"/>
                    <xsd:element name="isWearableItemDisposable" type="NonbinaryLogicCode"/>
                    <xsd:element name="seasonCalendarYear" type="xsd:gYear"/>
                    <xsd:element name="seasonName" type="xsd:string"/>
                    <xsd:element name="seasonParameter" type="SeasonParameterCode"/>
                    <xsd:element name="size" type="SizeDetails"/>
                    <xsd:element name="sportingActivityType" type="SportingActivityTypeCode"/>
                    <xsd:element name="styleDescription" type="xsd:string"/>
                    <xsd:element name="targetConsumerAge" type="xsd:string"/>
                    <xsd:element name="targetConsumerGender" type="TargetConsumerGenderCode"/>
                    <xsd:element name="textileMaterialContent" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
