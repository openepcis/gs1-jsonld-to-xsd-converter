<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gs1="https://gs1.org/voc">
    <xsd:complexType name="AdditionalProductClassificationDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A product classification for the product other than the Global Product Classification(GPC brick value).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="additionalProductClassificationCode" type="xsd:string"/>
            <xsd:element name="additionalProductClassificationCodeDescription" type="xsd:string"/>
            <xsd:element name="additionalProductClassificationValue" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="AdditiveDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A set of details about one of the additives within the product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="additiveLevelOfContainment" type="LevelOfContainmentCode"/>
            <xsd:element name="additiveName" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="AllergenDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Details of an allergen for a product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="allergenLevelOfContainmentCode" type="LevelOfContainmentCode"/>
            <xsd:element name="allergenType" type="AllergenTypeCode"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="AuthenticityDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A set of details about covert or overt security features that may contribute to checking the authenticity of a product or product
                instance.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="authenticitySecurityFeatureInstructions" type="xsd:string"/>
            <xsd:element name="authenticitySecurityFeatureInstructionsURL" type="xsd:anyURI"/>
            <xsd:element name="authenticitySecurityFeatureRegularExpression" type="xsd:string"/>
            <xsd:element name="authenticitySecurityFeatureType" type="SecurityMarking"/>
            <xsd:element name="authenticitySecurityFeatureValue" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="AwardPrizeDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Describes a prize or award won by a product or organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="awardPrizeCode" type="xsd:string"/>
            <xsd:element name="awardPrizeCountryCode" type="Country"/>
            <xsd:element name="awardPrizeDescription" type="xsd:string"/>
            <xsd:element name="awardPrizeJury" type="xsd:string"/>
            <xsd:element name="awardPrizeName" type="xsd:string"/>
            <xsd:element name="awardPrizeYear" type="xsd:gYear"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Beverage">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Any potable liquid.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="FoodBeverageTobaccoProduct">
                <xsd:sequence>
                    <xsd:element name="alcoholicBeverageSubregion" type="xsd:string"/>
                    <xsd:element name="beverageVintage" type="xsd:string"/>
                    <xsd:element name="isCarbonated" type="NonbinaryLogicCode"/>
                    <xsd:element name="isDecaffeinated" type="NonbinaryLogicCode"/>
                    <xsd:element name="isFromConcentrate" type="NonbinaryLogicCode"/>
                    <xsd:element name="isVintage" type="NonbinaryLogicCode"/>
                    <xsd:element name="percentageOfAlcoholByVolume" type="xsd:float"/>
                    <xsd:element name="vintner" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="Brand">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Information on brands and sub-brands for a product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="brandName" type="xsd:string"/>
            <xsd:element name="subBrandName" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="CertificationDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Certification issued by a certifying body to a certification subject (Organization, Place, Product).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="certificationAgency" type="xsd:string"/>
            <xsd:element name="certificationAgencyURL" type="Organization"/>
            <xsd:element name="certificationAuditDate" type="xsd:date"/>
            <xsd:element name="certificationEndDate" type="xsd:date"/>
            <xsd:element name="certificationIdentification" type="xsd:string"/>
            <xsd:element name="certificationStandard" type="xsd:string"/>
            <xsd:element name="certificationStartDate" type="xsd:date"/>
            <xsd:element name="certificationStatement" type="xsd:string"/>
            <xsd:element name="certificationStatus" type="CertificationStatus"/>
            <xsd:element name="certificationSubject" type="xsd:anyType"/>
            <xsd:element name="certificationType" type="xsd:anyType"/>
            <xsd:element name="certificationURI" type="xsd:anyURI"/>
            <xsd:element name="certificationValue" type="xsd:string"/>
            <xsd:element name="initialCertificationDate" type="xsd:date"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Clothing">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A product that is worn on the body.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="WearableProduct">
                <xsd:sequence>
                    <xsd:element name="clothingCut" type="xsd:string"/>
                    <xsd:element name="collarType" type="xsd:string"/>
                    <xsd:element name="isMaternity" type="NonbinaryLogicCode"/>
                    <xsd:element name="textileMaterial" type="TextileMaterialDetails"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ColourCodeDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A set of colour code details (colour code, party controlling the code list) for the product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="colourCodeList" type="ColourCodeListCode"/>
            <xsd:element name="colourCodeValue" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="ContactPoint">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Information on an individual or department acting as point of contact for an organiation.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="availableLanguage" type="xsd:string"/>
            <xsd:element name="contactTitle" type="xsd:string"/>
            <xsd:element name="contactType" type="xsd:string"/>
            <xsd:element name="email" type="xsd:string"/>
            <xsd:element name="faxNumber" type="xsd:string"/>
            <xsd:element name="responsibility" type="xsd:string"/>
            <xsd:element name="telephone" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Country">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Country is a complex data type that indicates a country and a country subdivision.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="countryCode" type="xsd:string"/>
            <xsd:element name="countrySubdivisionCode" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="DietTypeCodeDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A set of diet type code details (diet type code and diet type sub code) for the product.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="dietType" type="DietTypeCode"/>
            <xsd:element name="dietTypeSubcode" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="Discount">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Provides information on a discount applicable to an offer for example 2 percent.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="discountRepeatsPerMultipleMinimum" type="xsd:boolean"/>
            <xsd:element name="discountType" type="DiscountTypeCode"/>
            <xsd:element name="eligibleTradeChannel" type="TradeChannelCode"/>
            <xsd:element name="exactDiscountAmount" type="PriceSpecification"/>
            <xsd:element name="exactDiscountPercentage" type="xsd:float"/>
            <xsd:element name="exclusionDescription" type="xsd:string"/>
            <xsd:element name="isOnlyAvailableThroughRetailer" type="xsd:boolean"/>
            <xsd:element name="isOnlyWithMailingListSignup" type="xsd:boolean"/>
            <xsd:element name="isOnlyWithPaymentCard" type="PaymentMethod"/>
            <xsd:element name="isOnlyWithRetailerLoyaltyCard" type="xsd:boolean"/>
            <xsd:element name="isOnlyWithRetailerPaymentCard" type="xsd:boolean"/>
            <xsd:element name="isPromoterExclusiveOffer" type="xsd:boolean"/>
            <xsd:element name="maximumDiscountAmount" type="PriceSpecification"/>
            <xsd:element name="maximumDiscountPercentage" type="xsd:float"/>
            <xsd:element name="maximumQualifyingItems" type="xsd:integer"/>
            <xsd:element name="maximumQualifyingSpend" type="PriceSpecification"/>
            <xsd:element name="minimumDiscountAmount" type="PriceSpecification"/>
            <xsd:element name="minimumDiscountPercentage" type="xsd:float"/>
            <xsd:element name="minimumQualifyingItems" type="xsd:integer"/>
            <xsd:element name="minimumQualifyingSpend" type="PriceSpecification"/>
            <xsd:element name="offerRedemptionType" type="OfferRedemptionTypeCode"/>
            <xsd:element name="offerRestrictionDescription" type="xsd:string"/>
            <xsd:element name="qualifyingBrandName" type="xsd:string"/>
            <xsd:element name="qualifyingGPCs" type="xsd:string"/>
            <xsd:element name="qualifyingProductCategoryDescription" type="xsd:string"/>
            <xsd:element name="qualifyingProductClassificationCode" type="AdditionalProductClassificationDetails"/>
            <xsd:element name="qualifyingProductGTINs" type="xsd:string"/>
            <xsd:element name="qualifyingSubBrandName" type="xsd:string"/>
            <xsd:element name="selectedProductsOnly" type="xsd:boolean"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="FoodAndBeveragePreparationInformation">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Food and Beverage Preparation Information is a complex data type that indicates a preparation state code and preparation
                instructions.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="convenienceLevelPercent" type="xsd:float"/>
            <xsd:element name="manufacturerPreparationCode" type="PreparationTypeCode"/>
            <xsd:element name="maximumOptimumConsumptionTemperature" type="QuantitativeValue"/>
            <xsd:element name="minimumOptimumConsumptionTemperature" type="QuantitativeValue"/>
            <xsd:element name="preparationCode" type="PreparationTypeCode"/>
            <xsd:element name="preparationConsumptionPrecautions" type="xsd:string"/>
            <xsd:element name="preparationInstructions" type="xsd:string"/>
            <xsd:element name="yield" type="ProductYieldDetails"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="FoodBeverageTobaccoIngredientDetails">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Food Beverage Tobacco Ingredient is a complex data type that includes an ingredient statement and details.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ingredientContentPercentage" type="xsd:float"/>
            <xsd:element name="ingredientName" type="xsd:string"/>
            <xsd:element name="ingredientSequence" type="xsd:integer"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="FoodBeverageTobaccoProduct">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A food, beverage or tobacco product.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Product">
                <xsd:sequence>
                    <xsd:element name="allergenSpecificationAgency" type="xsd:string"/>
                    <xsd:element name="allergenSpecificationName" type="xsd:string"/>
                    <xsd:element name="allergenStatement" type="xsd:string"/>
                    <xsd:element name="biotinPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="calciumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="carbohydratesPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="chloridePerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="cholesterolPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="chromiumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="copperPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="dietCode" type="DietTypeCodeDetails"/>
                    <xsd:element name="dietTypeDescription" type="xsd:string"/>
                    <xsd:element name="drainedWeight" type="QuantitativeValue"/>
                    <xsd:element name="energyFromFatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="energyPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="fatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="fibrePerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="fluoridePerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="folicAcidPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="foodBeverageRefrigerationClaim" type="FoodBeverageRefrigerationClaimCode"/>
                    <xsd:element name="foodBeverageTargetUse" type="FoodBeverageTargetUseCode"/>
                    <xsd:element name="geneticallyModifiedDeclaration" type="LevelOfContainmentCode"/>
                    <xsd:element name="hasAllergen" type="AllergenDetails"/>
                    <xsd:element name="ingredient" type="FoodBeverageTobaccoIngredientDetails"/>
                    <xsd:element name="ingredientOfConcern" type="xsd:string"/>
                    <xsd:element name="ingredientStatement" type="xsd:string"/>
                    <xsd:element name="iodinePerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="ironPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="irradiatedCode" type="NonbinaryLogicCode"/>
                    <xsd:element name="isInstant" type="NonbinaryLogicCode"/>
                    <xsd:element name="isSliced" type="NonbinaryLogicCode"/>
                    <xsd:element name="juiceContentPercent" type="xsd:float"/>
                    <xsd:element name="magnesiumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="manganesePerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="molybdenumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="monounsaturatedFatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="niacinPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="numberOfServingsPerPackage" type="xsd:float"/>
                    <xsd:element name="numberOfServingsPerPackageMeasurementPrecision" type="MeasurementPrecisionCode"/>
                    <xsd:element name="numberOfServingsRangeDescription" type="xsd:string"/>
                    <xsd:element name="nutrientBasisQuantity" type="QuantitativeValue"/>
                    <xsd:element name="nutrientBasisQuantityType" type="NutrientBasisQuantityCode"/>
                    <xsd:element name="nutrientMeasurementPrecision" type="MeasurementPrecisionCode"/>
                    <xsd:element name="nutritionalClaim" type="NutritionalClaimTypeCode"/>
                    <xsd:element name="nutritionalClaimStatement" type="xsd:string"/>
                    <xsd:element name="packagingMarkedDietAllergenType" type="PackagingMarkedDietAllergenCode"/>
                    <xsd:element name="packagingMarkedFreeFrom" type="PackagingMarkedFreeFromCode"/>
                    <xsd:element name="pantothenicAcidPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="phosphorusPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="polyolsPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="polyunsaturatedFatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="potassiumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="preparationInformation" type="FoodAndBeveragePreparationInformation"/>
                    <xsd:element name="preservationTechnique" type="PreservationTechniqueCode"/>
                    <xsd:element name="proteinPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="reheatingClaim" type="NonbinaryLogicCode"/>
                    <xsd:element name="riboflavinPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="saltPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="saturatedFatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="seleniumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="servingSize" type="QuantitativeValue"/>
                    <xsd:element name="servingSizeDescription" type="xsd:string"/>
                    <xsd:element name="servingSuggestion" type="xsd:string"/>
                    <xsd:element name="sodiumPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="sourceAnimal" type="SourceAnimalCode"/>
                    <xsd:element name="starchPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="sugarsPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="thiaminPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="transFatPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminAPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminB12PerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminB6PerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminCPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminDPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminEPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="vitaminKPerNutrientBasis" type="NutritionMeasurementType"/>
                    <xsd:element name="zincPerNutrientBasis" type="NutritionMeasurementType"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="Footwear">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Outerwear that is worn on the feet such as shoes or boots.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="WearableProduct">
                <xsd:sequence>
                    <xsd:element name="footwearFasteningType" type="FootwearFasteningTypeCode"/>
                    <xsd:element name="footwearUpperType" type="UpperTypeCode"/>
                    <xsd:element name="upperMaterialType" type="TextileMaterialDetails"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="FruitsVegetables">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Contains properties related specifically to fruit and vegetable products.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="FoodBeverageTobaccoProduct">
                <xsd:sequence>
                    <xsd:element name="isPittedStoned" type="NonbinaryLogicCode"/>
                    <xsd:element name="isSeedless" type="NonbinaryLogicCode"/>
                    <xsd:element name="isShelledPeeled" type="NonbinaryLogicCode"/>
                    <xsd:element name="isWashedReadyToEat" type="NonbinaryLogicCode"/>
                    <xsd:element name="maturationMethod" type="MaturationMethodCode"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="GeoCoordinates">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The geographic coordinates of a place or event.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="coordinateReferenceSystem" type="xsd:anyURI"/>
            <xsd:element name="elevation" type="xsd:string"/>
            <xsd:element name="latitude" type="xsd:string"/>
            <xsd:element name="longitude" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="GeoShape">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">The geographic shape of a place. A GeoShape can be described using several properties whose values are based on latitude/longitude
                pairs. Either whitespace or commas can be used to separate latitude and longitude; whitespace should be used when writing a list of several such points.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="circle" type="xsd:string"/>
            <xsd:element name="line" type="xsd:string"/>
            <xsd:element name="polygon" type="xsd:string"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="LocationID_Details">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Government bodies, trade organisations, and other parties issue identifiers that are associated to locations. Linking these identifiers
                to GLN and one another supports consolidating records, mapping related, collaborative identifiers, enhances search ability and enables more efficient transitions
                between identifiers. This class provides a mechanism for connecting and sharing location identifiers.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="locationID" type="xsd:string"/>
            <xsd:element name="locationID_Qualifier" type="xsd:string"/>
            <xsd:element name="locationID_Type" type="LocationID_Type"/>
            <xsd:element name="locationID_URI" type="xsd:anyURI"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="LocationStatusHistory">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Over the lifetime of a location, it may go through periods of being active and inactive. gs1:LocationStatusHistory allows changes in
                status to be communicated in advance and tracked over time.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="locationStatus" type="StatusType"/>
            <xsd:element name="statusTimestamp" type="xsd:dateTime"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:anyURI"/>
    </xsd:complexType>
    <xsd:complexType name="MeatPoultry">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Meat and poultry products.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="FoodBeverageTobaccoProduct">
                <xsd:sequence>
                    <xsd:element name="anatomicalForm" type="AnatomicalFormCode"/>
                    <xsd:element name="bonelessClaim" type="NonbinaryLogicCode"/>
                    <xsd:element name="meatPoultryType" type="xsd:string"/>
                    <xsd:element name="minimumMeatPoultryContent" type="QuantitativeValue"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="MilkButterCreamYogurtCheeseEggsSubstitutes">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Milk butter cream yogurts cheese eggs and any substitutes for these products.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="FoodBeverageTobaccoProduct">
                <xsd:sequence>
                    <xsd:element name="cheeseFirmness" type="CheeseFirmnessCode"/>
                    <xsd:element name="cheeseMaturationPeriodDescription" type="xsd:string"/>
                    <xsd:element name="fatInMilkContent" type="xsd:float"/>
                    <xsd:element name="fatpercentageInDryMatter" type="xsd:float"/>
                    <xsd:element name="isHomogenised" type="NonbinaryLogicCode"/>
                    <xsd:element name="isRindEdible" type="NonbinaryLogicCode"/>
                    <xsd:element name="sharpnessOfCheese" type="SharpnessOfCheeseCode"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="NutritionMeasurementType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">A class providing nutritional value and intake percent.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="QuantitativeValue">
                <xsd:sequence>
                    <xsd:element name="dailyValueIntakePercent" type="xsd:float"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="Offer">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">An offer to transfer some rights to an item or to provide a service,for example, an offer to sell tickets to an event, to rent the DVD
                of a movie, to stream a TV show over the internet, to repair a motorcycle, or to loan a book.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="acceptedPaymentMethod" type="PaymentMethod"/>
            <xsd:element name="availabilityEnds" type="xsd:dateTime"/>
            <xsd:element name="availabilityStarts" type="xsd:dateTime"/>
            <xsd:element name="availableAtOrFrom" type="Place"/>
            <xsd:element name="eligibleQuantity" type="QuantitativeValue"/>
            <xsd:element name="eligibleQuantityMaximum" type="QuantitativeValue"/>
            <xsd:element name="eligibleQuantityMinimum" type="QuantitativeValue"/>
            <xsd:element name="itemOffered" type="Product"/>
            <xsd:element name="offerDescription" type="xsd:string"/>
            <xsd:element name="offerDiscount" type="Discount"/>
            <xsd:element name="offerRedemptionURL" type="ReferencedFileDetails"/>
